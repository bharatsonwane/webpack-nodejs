{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "node": "10.11.0",
                    "esmodules": false
                }
            }
        ]
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-decorators",
            {
                "legacy": true
            }
        ],
        "@babel/plugin-proposal-class-properties"
    ]
}